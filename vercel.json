{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/santiasTravelFront/browser",
  "framework": null,
  "installCommand": "npm install",
  "devCommand": "npm start",
  "trailingSlash": false,
  "headers": [
    {
      "source": "/ngsw-worker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/ngsw.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*\\.(?:js|css|ico|png|jpg|jpeg|gif|svg|webp|woff|woff2|ttf|eot|otf))$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api|assets|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
